{
    "name": "goksagun/redis-orm-bundle",
    "description": "Redis orm bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Burak Bolat",
            "email": "brkblt@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "doctrine redis orm bundle"
    ],
    "require": {
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*"
    },
    "autoload": {
        "psr-4": { "Goksagun\\RedisOrmBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
